###  https://rs.tdwg.org/bdqffdq/terms/NOT_ISSUE
bdqffdq:NOT_ISSUE rdf:type owl:NamedIndividual ,
                           bdqffdq:ResponseResult ;
                  owl:differentFrom bdqffdq:POTENTIAL_ISSUE ;
                  dcterms:issued "2024-09-30"^^xsd:date ;
